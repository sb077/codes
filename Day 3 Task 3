class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& a) {
        vector<vector<int> > s(a[0].size(),vector<int>());
        for(int i=0;i<a.size();i++){
            for(int j=0;j<a[i].size();j++){
                s[j].push_back(a[i][j]);
               
            }
        }
        
        return s;
    }
};
