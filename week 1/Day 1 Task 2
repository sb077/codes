//missing number 

#include<bits/stdc++.h>
using namespace std;

int main(){
    int t;
    cin>>t;
    for(int k=0;k<t;k++){
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n-1;i++)
        cin>>a[i];
    int x=((n)*(n+1))/2;
    for(int j=0;j<n-1;j++){
        x=x-a[j];
    }
    cout<<x<<endl;;
    }
    return 0;
}
